	<queryString>
		<![CDATA[SELECT	Distinct O.fecha,O.hora,o.nro_Orden,pr.id_producto AS C贸digo,PR.descripcion,PPP.precio,DOC.cantidad, (PPP.precio * DOC.cantidad) AS Total,p.descripcion as Proveedor,EDP.detalle AS Estado
FROM OrdenesDeCompra O JOIN Proveedores P ON O.id_Proveedor=P.codigo_proveedor 
JOIN DetalleOrdenesDeCompra DOC ON O.nro_Orden=DOC.nro_Orden
JOIN Productos PR ON DOC.id_producto=PR.id_producto
JOIN ProductoPorProveedor PPP ON PPP.id_producto = PR.id_producto
JOIN Proveedores ON PPP.codigo_proveedor = P.codigo_proveedor
JOIN EstadosDePedido EDP ON O.estado=EDP.cod_estado
WHERE	O.nro_Orden=(SELECT MAX(nro_Orden) FROM OrdenesDeCompra)
GROUP BY O.fecha,O.hora,pr.id_producto,PR.descripcion,PPP.precio,DOC.cantidad,p.descripcion,EDP.detalle,o.nro_Orden]]>
	</queryString>
